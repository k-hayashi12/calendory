<div class="container">
	<div class="row">
		<div class="col-lg-1">
		</div>

		<div class="col-lg-9">
				<%= form_for(@user) do |f| %>
					<%= f.file_field :user_image_id, class: "form" %>
					<label class="font">氏名</label><%= f.text_field :name, class: "form1" %><br/>
					<label class="font">メールアドレス</label><%= f.text_field :email, class: "form2" %><br/>
					<label class="font">検索用ID</label><%= f.text_field :search_number, class: "form3" %><br/>

					<%= f.submit "編集完了", class: "form4 btn btn-primary"%>
				<% end %>
		</div>
	</div>
</div>