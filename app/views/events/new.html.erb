<%= form_with(model: @event, local: true) do |form| %>

  <div class="event_title">
    <%= form.label :イベント名 %>
    <%= form.text_field :event_title %>
  </div>

    <div class="time">
    <%= form.label :日付 %>
    <%= form.date_select :start_time %>
  </div>

  <div class="submit">
    <%= form.submit %>
  </div>

<% end %>